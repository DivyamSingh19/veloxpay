{
  "name": "velox",
  "private": true,
  "scripts": {
    "build": "turbo run build",
    "dev": "turbo run dev",
    "lint": "turbo run lint",
    "format": "prettier --write \"**/*.{ts,tsx,md}\"",
    "check-types": "turbo run check-types",
    "db:migrate": "drizzle-kit migrate",
    "db:generate": "drizzle-kit generate"
  },
  "devDependencies": {
    "prettier": "^3.5.0",
    "prisma": "^6.9.0",
    "turbo": "^2.4.4",
    "typescript": "^5.9.3"
  },
  "engines": {
    "node": ">=18"
  },
  "packageManager": "npm@11.1.0",
  "workspaces": [
    "apps/*",
    "packages/*"
  ],
  "dependencies": {
    "@neondatabase/serverless": "^1.0.1",
    "@prisma/client": "^6.9.0",
    "@tailwindcss/postcss": "^4.0.12",
    "bcrypt": "^6.0.0",
    "cookie-parser": "^1.4.7",
    "drizzle-kit": "^0.31.7",
    "hono": "^4.8.5",
    "jsonwebtoken": "^9.0.2",
    "postcss": "^8.5.3",
    "tailwindcss": "^4.0.12"
  }
}
